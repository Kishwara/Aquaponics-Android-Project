<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lenovo.aquaponicsassistant">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".GettingStartedActivity1" />
        <activity android:name=".OptionsActivity" />
        <activity android:name=".WhatIsAquaponicsActivity" />
        <activity android:name=".BenefitsOfAquaponics" />
        <activity android:name=".SeedTrackerActivity" />
        <activity android:name=".FAQActivity" />
        <activity android:name=".CommunityActivity" />
        <activity android:name=".OnlineResourcesActivity" />
        <activity android:name=".RequirementsActivity" />
        <activity android:name=".AlarmReceiver" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".ChatActivity" />
        <activity android:name=".DiscussionActivity"></activity>
    </application>

</manifest>